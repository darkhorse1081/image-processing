function[binary] = ExtractImage(colour)



% b
binary = uint8(zeros(size(colour(:,:,1))));
[row,col] = size(colour(:,:,1));

for i = 1:row
    for j = 1:col
        red = colour(i,j,1);
        green = colour(i,j,2);
        blue = colour(i,j,3);

        total = double(red)+double(green)+double(blue);

        if mod(total,2) == 0
            binary(i,j) = 0;
        elseif mod(total,2) == 1
             binary(i,j) = 255;
        end

    end
end


end